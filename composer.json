{
    "name": "mazarini/pagination-bundle",
    "type": "symfony-bundle",
    "description": "Pagination bundle for symfony and doctrine orm.",
    "keywords": [
        "frontend"
    ],
    "homepage": "https://github.com/mazarini/pagination-bundle",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Mazarini",
            "email": "mazarini@protonmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3|^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "mazarini/bootstrap": "^1.0",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/flex": "^1.4",
        "symfony/form": "^4.3|^5.0",
        "symfony/orm-pack": "^1.0",
        "symfony/security-csrf": "^4.3|^5.0",
        "symfony/validator": "^4.3|^5.0"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.2",
        "doctrine/doctrine-fixtures-bundle": "^3.2",
        "symfony/maker-bundle": "^1.14",
        "symfony/test-pack": "^1.0",
        "symfony/web-profiler-bundle": "^4.3|^5.0",
        "symfony/web-server-bundle": "^4.3|^5.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Mazarini\\PaginationBundle\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "App\\": "src/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "symfony": {
            "allow-contrib": true,
            "require": "5.0.*"
        }
    },
    "minimum-stability": "stable"
}
